{
  "manifest_version": 2,
  "name": "FluxTab",
  "version": "0.1",
  "minimum_chrome_version": "23",
  "omnibox": { "keyword" : "." },
  "background": {
    "persistent": false,
    "page": "background.html"
  },
  "chrome_url_overrides" : {
    "newtab": "newtab.html"
  },
  "permissions": [
    "storage",
    "file:///*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
